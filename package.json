{
  "name": "tempo-influencer",
  "private": true,
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=server\" \"npm run dev --workspace=client\"",
    "build": "npm run build --workspace=server && npm run build --workspace=client",
    "start": "NODE_ENV=production node server/dist/index.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
